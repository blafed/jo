log<T>(x T){
    cline {
        char buf[128];
        long long len;
    }
    match T {
        int: cline {
            len = jo_stry_int(x, buf);
        }
        float: cline {
            len = jo_stry_float(x, buf);
        }
        bool: cline {
            len = jo_stry_bool(x, buf);
        }
        str: cline {printf(x); return;}
        else: ???
    }

    cline {
        printf("%.*s", (int)len, buf);
    }
}